{
  "mcpServers": {
    "prompts": {
      "command": "node",
      "args": ["$PATH_TO_REPOSITORY/dist/index.js"],
      "env": {
        "PROMPTS_DIR": "$PATH_TO_REPOSITORY/src/prompts"
      },
      "transport": "stdio"
    },
    "prompts": {
      "command": "npx",
      "args": ["-y", "@h16rkim/mcp-prompt-server@latest"],
      "env": {
        "PROMPTS_DIR": "$PATH_TO_REPOSITORY/src/prompts"
      },
      "transport": "stdio"
    }
  }
}
